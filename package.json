{
  "name": "ll-tools",
  "displayName": "LL Tools",
  "version": "1.0.0",
  "description": "LL Tools",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build:backend && electron . --dev",
    "prebuild": "rimraf out && rimraf dist && rimraf frontend/.next && rimraf frontend/out",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "next build frontend",
    "build:backend": "tsc -p backend",
    "postinstall": "electron-builder install-app-deps",
    "dist": "npm run build && npx electron-builder",
    "update:readme": "tsx scripts/update-readme.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/material-nextjs": "^6.4.3",
    "@tabler/icons-react": "^3.30.0",
    "app-root-path": "^3.1.0",
    "electron-log": "^5.3.0",
    "react-to-print": "^3.0.5",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "next": "^15.1.7",
    "postcss": "^8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
